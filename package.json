{
  "name": "eslint-plugin-no-use-extend-native",
  "version": "0.3.4",
  "description": "ESLint plugin to prevent use of extended native objects",
  "scripts": {
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ./ --ignore-pattern node_modules/ --ignore-pattern lib/ --ignore-pattern ava/",
    "test": "npm run lint && nyc ava"
  },
  "repository": "dustinspecker/eslint-plugin-no-use-extend-native",
  "keywords": [
    "eslint",
    "eslintplugin",
    "extend",
    "native",
    "prototype"
  ],
  "author": "Dustin Specker",
  "license": "MIT",
  "files": [
    "index.js",
    "rules"
  ],
  "dependencies": {
    "is-get-set-prop": "^1.0.0",
    "is-js-type": "^1.0.0",
    "is-proto-prop": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-eslint": "^5.0.0",
    "coveralls": "^2.11.2",
    "eslint": "^2.0.0",
    "eslint-config-dustinspecker": "^0.4.1",
    "eslint-path-formatter": "^0.1.1",
    "eslint-plugin-new-with-error": "^1.1.0",
    "nyc": "^6.1.1"
  }
}
